<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <!-- Suppress false positives for development dependencies -->
    <suppress>
        <notes><![CDATA[
            Suppress Spring Boot test dependencies that are not used in production
        ]]></notes>
        <gav regex="true">^org\.springframework\.boot:spring-boot-starter-test:.*$</gav>
    </suppress>
    
    <!-- Suppress QueryDSL annotation processor vulnerabilities -->
    <suppress>
        <notes><![CDATA[
            QueryDSL annotation processor is only used during build time
        ]]></notes>
        <gav regex="true">^com\.querydsl:querydsl-apt:.*$</gav>
    </suppress>
</suppressions>